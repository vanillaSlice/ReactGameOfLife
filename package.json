{
  "name": "react-game-of-life",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "node-sass-chokidar": "^0.0.3",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "^1.0.17",
    "reset-css": "^2.2.1"
  },
  "devDependencies": {
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "gh-pages": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-scss": "^2.2.0"
  },
  "homepage": "https://vanillaslice.github.io/ReactGameOfLife",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "run-p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test:lint:js": "eslint --ext=js --ext=jsx ./src",
    "test:lint:css": "stylelint './src/**/*.scss'",
    "test:unit": "react-scripts test --env=jsdom",
    "test": "run-s build-css test:**",
    "eject": "react-scripts eject"
  }
}